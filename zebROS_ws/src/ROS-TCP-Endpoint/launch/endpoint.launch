<launch>
    <arg name="tcp_ip" default="0.0.0.0"/>
    <arg name="tcp_port" default="5810"/>

	<node pkg="tf2_ros" type="static_transform_publisher" name="link_toquest" args="0.2 0 0 0 0 0 1 base_link quest_hardware_offset" />

    <node name="unity_endpoint" pkg="ros_tcp_endpoint" type="default_server_endpoint.py" output="screen">
        <param name="tcp_ip" type="string" value="$(arg tcp_ip)"/>
        <param name="tcp_port" type="int" value="$(arg tcp_port)"/>
    </node>
        <!-- Launch the PosRot to TF publisher node -->
    <node name="pos_rot_to_tf_publisher" pkg="ros_tcp_endpoint" type="vis_oculus_msg.py" output="screen" />

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ros_tcp_endpoint)/launch/tf2default.rviz" output="screen" />
</launch>
